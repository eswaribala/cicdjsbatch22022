pipeline{
  agent any
  tools {
    nodejs 'node'
    maven 'maven'
  }
  stages{
        stage('build'){
                      steps{
                          script{
                                 try{
                                      
                                      sh "node app.js"
                                 }
                                 catch(all){
                                      echo "Test stage Failed"
                                      
                                      exit 1
                                 }
                          }
                     }
        }

        
        
  }      
}
